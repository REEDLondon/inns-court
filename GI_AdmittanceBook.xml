            <date when-iso="1594-02-02" cert="high"><supplied>2 February</supplied></date>
            <!-- DKJ 2018-01-28: would someone confirm that this should be 1594  -->
            <seg ana="docu:RLGIAB" corresp="docu:IC1 p. 124">RLGIAB</seg>
         </head>
