   <text ana="taxon:grays_inn" type="record">
      <body xml:lang="eng">
         <head>
            <date from-iso="1586" to-iso="1587">1587-8</date>
            <seg ana="taxon:RLGILB01" corresp="taxon:RPCIC1 p. 111">RLGILB01</seg></head>
         <div type="transcription">

            <div>
               <head>f 13<supplied><date from-iso="1586-11-21" to-iso="1587-11-22">(21 November
                        1586–22 November 1587)</date> (Treasurer’s allowances)</supplied></head>
               <pb n="13" type="folio"/>
               <gap reason="omitted"/>
               <table>
                  <row>
                     <cell>It<ex>e</ex>m payd in hillarie terme for the mus<ex>i</ex>cions in
                           chr<ex>isten</ex>mas <del>d</del> now:</cell>
                     <cell>iiij li. vj s.</cell>
                  </row>
               </table>
               <gap reason="omitted"/>
            </div>

            <div>
               <head>f 17<date from-iso="1587-09-29" to-iso="1588-09-28"><supplied>(29 September
                        1587–28 September 1588) (Collector’s disbursements)</supplied></date><ptr
                     type="endnote" target="RLE00095"/></head>
               <pb n="17" type="folio"/>
               <gap reason="omitted"/>
               <table>
                  <row>
                     <cell>It<ex>e</ex>m delyvered to <persName n="John Buck">Iohn Buck</persName> 9
                           ffebr<ex>uary</ex> ffor and toward<ex>es</ex> the charges the gentlemen
                        were att in christenmas</cell>
                     <cell>x li. vij s. xj d.</cell>
                  </row>
                  <row>
                     <cell>It<ex>e</ex>m deliu<ex>er</ex>ed to <persName n="Francis Flower">mr
                           fflower</persName> 18 ffebr<ex>uary</ex> ffor and toward<ex>es</ex> the
                        chardges of the Tragedie<note type="foot">Tragedie: <hi rend="italic">presumably</hi> The Misfortunes of
                           Arthur</note></cell>
                     <cell>xx li.</cell>
                  </row>
                  <row>
                     <cell>It<ex>e</ex>m delyvered more to <persName n="Francis Flower">mr
                           fflower</persName> 20 ffebr<ex>uary</ex> toward<ex>es</ex> the chardges
                        of the Tragedie</cell>
                     <cell>xx li.</cell>
                  </row>
               </table>
               <gap reason="omitted"/>
            </div>

            <div>
               <head>f 18v<date from-iso="1588-10-28" to-iso="1589-10-31"><supplied>(28 October
                        1588–31 October 1589)</supplied></date></head>
               <pb n="18v" type="folio"/>
               <gap reason="omitted"/>

               <table>
                  <row>
                     <cell>It<ex>em</ex> payd to the musicians for their ffee due the last
                        yere</cell>
                     <cell>xx s.</cell>
                  </row>
               </table>
               <gap reason="omitted"/>
            </div>
            
            <div>
               <head>f 23v<date when-iso="1588-11-13"><supplied>(13 November 1588) (Pensioner’s
                        accounts)</supplied></date><ptr type="endnote" target="RLE00095"/></head>
               <pb n="23v" type="folio"/>
               <gap reason="omitted"/>

               <table>
                  <row>
                     <cell>Ite<ex>m</ex> payd to <persName n="Thomas Wilson" role="carpenter">thomas
                           wilson</persName> the Carpender for x ffyrre Truncheuns<note
                           type="foot">Truncheuns: <hi rend="italic">or</hi> Trunchenns</note> at the play in michelmas
                        terme</cell>
                     <cell>ij s. vj d.</cell>
                  </row>
                  <row>
                     <cell>Ite<ex>m</ex> for Torches at that tyme</cell>
                     <cell>viij s.</cell>
                  </row>
               </table>
               <gap reason="omitted"/>

            </div>
         </div>
      </body>
   </text>

