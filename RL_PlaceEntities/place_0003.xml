                  <geo
                     decls="#WGS">51.50831 -0.09609</geo>
               </location>
               <note
                  type="general"
                  xml:lang="ENG"> As referred to in REED London: Inns of Court collection</note>
               <note
                  type="project-specific"
                  xml:lang="ENG">
                  <respons
                     locus="value">
                     <desc>
                        <orgName
                           ref="URI">REED</orgName>
                     </desc>
                  </respons>
               </note>
               
               <idno type="Wikidata" cert="high" sameAs="https://www.wikidata.org/wiki/Q2660320"/>
